{
    "name": "avexsoft/laravel-sidepress",
    "description": "Let Wordpress share the same domain root, side-by-side, with Laravel",
    "keywords": [
        "Laravel",
        "Wordpress",
        "Sidepress"
    ],
    "homepage": "https://github.com/avexsoft/laravel-sidepress",
    "license": "MIT",
    "authors": [
        {
            "name": "Avexsoft",
            "homepage": "https://github.com/avexsoft"
        }
    ],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Avexsoft\\Sidepress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avexsoft\\Sidepress\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avexsoft\\Sidepress\\SidepressServiceProvider"
            ],
            "aliases": {
                "Sidepress": "Avexsoft\\Sidepress\\Facades\\Sidepress"
            }
        }
    }
}